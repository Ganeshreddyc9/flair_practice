    "import math\n",
    "def square(func):\n",
    "    def number(num):\n",
    "        result = func(num)\n",
    "        return math.sqrt(result)\n",
    "    return number\n",
    "@square\n",
    "def square_of_number(num):\n",
    "    return num * num\n",
    "    "
